{
  "name": "@suseejs/bundler",
  "version": "0.1.0",
  "description": "Mini Bundler",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "tsx opt/index.ts",
    "fmt": "biome format --write",
    "lint": "biome check src --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suseejs/bundler.git"
  },
  "keywords": [
    "TypeScript"
  ],
  "author": {
    "name": "Pho Thin Mg",
    "email": "phothinmg@disroot.org",
    "url": "https://phothinmg.github.io/"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/suseejs/bundler/issues"
  },
  "homepage": "https://github.com/suseejs/bundler#readme",
  "dependencies": {
    "@phothinmaung/resolves": "^1.0.0",
    "@suseejs/anonymous": "^0.1.3",
    "@suseejs/check": "^0.1.3",
    "@suseejs/dependencies": "^0.1.1",
    "@suseejs/duplicates": "^0.1.2",
    "@suseejs/transformer": "^0.1.0",
    "@suseejs/tsconfig": "^0.1.1",
    "@suseejs/types": "^0.1.1",
    "@suseejs/utils": "^0.1.2",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "tsx": "^4.21.0"
  }
}
